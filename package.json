{
  "name": "exhibit-builder-minify-html",
  "description": "Minifies HTML files using minimize",
  "version": "0.0.0",
  "author": "Callum Locke",
  "babel": {
    "stage": 0,
    "blacklist": [
      "regenerator",
      "es6.forOf",
      "es6.templateLiterals",
      "es6.constants",
      "es6.properties.computed"
    ],
    "optional": [
      "bluebirdCoroutines"
    ]
  },
  "dependencies": {
    "minimize": "^1.7.3"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "eslint": "^1.7.1",
    "eslint-config-exhibit": "^2.0.0",
    "eslint-plugin-babel": "^2.1.1",
    "rimraf": "^2.4.3"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "peerDependencies": {
    "exhibit": "^0.7.0"
  },
  "repository": "exhibitjs/builder-minify-html",
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist --source-maps",
    "buildwatch": "rimraf dist && babel src --out-dir dist --source-maps --watch",
    "prepublish": "npm run build",
    "test": "eslint ."
  }
}
